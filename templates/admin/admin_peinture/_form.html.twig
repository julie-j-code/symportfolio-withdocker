<section>

    {{ form_start(form) }}
    <div class="admin-form bg-transparent">
        <div class="row">
            <div class="col-md-6">
                <div class="form-group my-3">
                    {{ form_row (form.nom) }}
                </div>
                <div class="form-group my-3">
                    {{ form_row (form.hauteur) }}
                </div>
                <div class="form-group my-3">
                    {{ form_row (form.largeur) }}
                </div>
                <div class="form-group my-3">
                   

                    {{ form_row (form.enVente) }}
                </div>
                <div class="form-group my-3">
                    {{ form_row (form.prix) }}
                </div>
            </div>
            <div class="col-md-6">
                <div class="form-group my-3">
                    {{ form_row (form.dateRealisation) }}
                </div>

                <div class="form-group my-3">
                    {{ form_row (form.description, {'attr': {'class': 'form-control'}}) }}
                </div>
                <div class="form-group my-3">
                    {{ form_row (form.slug) }}
                </div>
                <div class="form-group my-3">
                    {{ form_label (form.file)}}
                    {{ form_widget (form.file)}}
                </div>
            </div>
        </div>

        <button class="btn btn-warning">{{ button_label|default('Save') }}</button>
    </div>
    {{ form_end(form) }}
</section>